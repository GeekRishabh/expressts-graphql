    "@types/bcrypt-nodejs": "0.0.30",
    "@types/compression": "1.0.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.18",
    "@types/mongodb": "^3.3.1",
    "@types/mongoose": "^5.5.17",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "typescript": "^3.6.3"
