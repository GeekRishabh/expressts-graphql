{
  "name": "expressts-graphql",
  "version": "1.0.0",
  "description": "API server built using TypeScript with GraphQL",
  "main": "dist",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "server:test": "TESTING=true tsc --watch & NODE_ENV=development nodemon dist",
    "test": "TESTING=true jest ./tests/index.test.ts",
    "start": "tsc --watch & NODE_ENV=development nodemon dist",
    "dev": "tsc --watch & NODE_ENV=development nodemon dist",
    "start:debug": "cross-env DEBUG=expressts-graphql:* yarn start",
    "lint": "./node_modules/.bin/tslint -c tslint.json 'src/**/**.ts'",
    "lint:watch": "yarn lint -- --watch"
  },
  "author": "GeekRishabh | GeekyAnts <geekrishabh@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@typegoose/typegoose": "^6.0.3",
    "apollo-server": "^2.9.5",
    "apollo-server-express": "^2.9.5",
    "assert": "^2.0.0",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.5",
    "compression": "^1.7.4",
    "connect": "^3.7.0",
    "connect-mongo": "^3.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "fcm-node": "^1.5.2",
    "firebase": "^7.1.0",
    "firebase-admin": "^8.6.0",
